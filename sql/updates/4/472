use strict;
use libdb;

sub DoUpdate($$$)
{
    my ($dbhandle, $dbname, $version) = @_;

    if (!DBSlotExists("image_updates", "updater_urn")) {
	DBQueryFatal("ALTER TABLE image_updates ADD ".
		     " `updater_urn` varchar(128) default NULL AFTER imageid");
    }
    return 0;
}

